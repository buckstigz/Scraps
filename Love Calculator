# print('Your new message is: ', newMessage) 



alphabet = 'abcdefghijklmnopqrstuvwxyz'
names = input('Enter the two names: ')
score = 0
for character in names:
  if character in alphabet:
    score += 2
  if character in 'lesbians':
    score += 11
  if character in 'gay':
    score += 4
  if character in 'love':
    score -= 3
total = score
if total >= 200:
  print('God Gays')
elif total >= 100:
  print('Gay Gays')
elif total <= 50:
  print('Good Gays')
else:
    print('Not in love :(')
    
print('Your love score is: ', score)
    


